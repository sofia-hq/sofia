  "author": "Nomos Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dowhiledev/nomos.git",
    "directory": "sdk/ts"
  },
  "homepage": "https://github.com/dowhiledev/nomos/tree/main/sdk/ts",
  "bugs": {
    "url": "https://github.com/dowhiledev/nomos/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "vitest": "^1.1.0",
    "nock": "^13.3.0",
    "typescript": "^5.3.3"
  }
