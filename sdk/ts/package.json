{
  "name": "nomos-sdk",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript SDK for interacting with Nomos agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nomos",
    "agent",
    "ai",
    "chatbot",
    "conversation",
    "typescript",
    "sdk",
    "api-client"
